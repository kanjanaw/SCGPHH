                formButton2.classList.add("d-none");
            } else if (mvtFormEl[i].getAttribute("id") == mvtSelectEl.value && (mvtFormEl[i].getAttribute("id") == 'Z15-1' || (mvtFormEl[i].getAttribute("id") == '202-1'))) {
