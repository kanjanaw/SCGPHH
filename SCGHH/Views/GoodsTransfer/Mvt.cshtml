    function changeForm() {
        for (let i = 0; i < mvtFormEl.length; i++) {
            if (mvtFormEl[i].getAttribute("id") == mvtSelectEl.value && (mvtFormEl[i].getAttribute("id") != 'Z15-1') && (mvtFormEl[i].getAttribute("id") != '202-1')) {
